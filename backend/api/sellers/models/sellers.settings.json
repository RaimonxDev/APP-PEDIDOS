{
  "kind": "collectionType",
  "collectionName": "sellers",
  "info": {
    "name": "Sellers"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "Nombres": {
      "type": "string",
      "required": true
    },
    "Apellidos": {
      "type": "string",
      "required": true
    },
    "Correo": {
      "type": "email",
      "required": true
    },
    "Foto": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    }
  }
}
